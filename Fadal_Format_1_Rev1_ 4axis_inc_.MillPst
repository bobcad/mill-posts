2003. Spindle speed + feed for rigid tap, unused
	Dim SPINSPEED
	Dim TPI
	Dim SPEEDSTR
	Dim FEEDSTR
	Dim RETSTR
	SPINSPEED = MILL_GetSpindleSpeed()
	TPI = MILL_GetThreadsPerInch()
	FEED = Round(SPINSPEED / TPI, 4)
	FEEDSTR = "F"&CStr(FEED)
	If SPINSPEED > 749 Then
		SPEEDSTR = "S"&CStr(SPINSPEED)&".2"
	Else
		SPEEDSTR = "S"&CStr(SPINSPEED)
	End If
	RETSTR = " "&SPEEDSTR&" "&FEEDSTR
	MILL_SetReturnString(RETSTR)

